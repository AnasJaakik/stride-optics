{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/backend/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 300,
      "memory": 2048
    }
  }
}
